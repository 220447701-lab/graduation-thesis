     	\begin{center}
	\@chapapp\ \thechapter\ \@chappos
	\par\vskip0.5\baselineskip
	\huge\bfseries#1%
	\end{center}
  \if@openright\cleardoublepage\else\clearpage\fi
   \else
     #1\relax
   \fi}\nobreak\vskip3\Cvs}
\def\@schapter#1{%
  \@makeschapterhead{#1}\@afterheading
}
